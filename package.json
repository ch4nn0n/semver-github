  "scripts": {
    "build:dev": "mkdir -p ./dist && webpack --config webpack.dev.js",
    "build:prod": "mkdir -p ./dist && webpack --config webpack.prod.js",
    "test": "mocha \"src/js/**/*.test.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ch4nn0n/semver-github.git"
  },
  "author": "Joshua Channon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ch4nn0n/semver-github/issues"
  },
  "homepage": "https://github.com/ch4nn0n/semver-github#readme",
  "dependencies": {
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.3",
    "create-file-webpack": "^1.0.0",
    "crx-webpack-plugin": "^0.1.5",
    "eslint": "^5.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
